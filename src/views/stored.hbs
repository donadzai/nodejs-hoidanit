<table class='table'>
    <thead>
        <tr>
            <th scope='col'>#</th>
            <th scope='col'>First Name</th>
            <th scope='col'>Last Name</th>
            <th scope='col'>Address</th>
        </tr>
    </thead>
    <tbody>
        {{#each users}}
            <tr>
                <th scope='row'>{{@index}}</th>
                <td>{{this.firstName}}</td>
                <td>{{this.lastName}}</td>
                <td>{{this.address}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>